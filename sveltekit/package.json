{
  "name": "svelte-config",
  "version": "1.0.0",
  "scripts": {
    "start": "node build",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
